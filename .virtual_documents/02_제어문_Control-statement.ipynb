coffee = 10

while True :
    money = int(input('돈을 넣어주세요:'))

    if money == 300:
        print(f'{money}원을 받았습니다. 커피를 드립니다.')
        coffee = coffee - 1
        print(f'남은 커피의 양은 {coffee}입니다.')
    elif money > 300:
        print(f'{money}원을 받았습니다. 잔돈 {money-300}원을 드립니다. 커피를 드립니다.')
        coffee = coffee - 1
        print(f'남은 커피의 양은 {coffee}입니다.')
    else :
        print(f'돈이 부족합니다. {money}원을 반환합니다.')
        coffee = coffee - 0
        print(f'남은 커피의 양은 {coffee}입니다.')

    if coffee == 0:
        print('커피가 품절되었습니다. 영업을 종료합니다.')
        break





