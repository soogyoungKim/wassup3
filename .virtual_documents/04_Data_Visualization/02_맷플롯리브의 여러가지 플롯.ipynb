ZZ = np.sin(np.sqrt(XX**2 + YY**2))


fig = plt.figure()
# ax = Axes3D(fig)
ax = fig.add_subplot(111, projection='3d')
ax.set_title("3D Surface Plot")

ax.plot_surface(XX, YY, ZZ, rstride=3, cstride=2, cmap='hot')
plt.show()


