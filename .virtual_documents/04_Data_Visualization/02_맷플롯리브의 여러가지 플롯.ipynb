def f(x, y): # z값 계산 합수
    return (1 - (x/2) + (x**5) + (y**3)) * np.exp(-x**2 - y**2)

n = 256
x = np.linspace(-3, 3, n)
y = np.linspace(-3, 3, n)

XX, YY = np.meshgrid(x, y) # 그리드 포인트 행렬값으로 변환
ZZ = f(XX, YY) # z값 계산

plt.title("Contour plots")
plt.contourf(XX, YY, ZZ, alpha=.75, cmap='jet') # 색깔로 표시
plt.contour(XX, YY, ZZ, colors='black') # 등고선만 표시
plt.show()





